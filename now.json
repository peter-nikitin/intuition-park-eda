{
    "version": 2,
    "name": "intuition-park-eda",
    "builds": [{
        "src": "package.json",
        "use": "@now/static-build",
        "config": {
            "distDir": "build"
        }
    }],
    "routes": [{
            "src": "/static/(.*)",
            "headers": {
                "cache-control": "s-maxage=31536000,immutable"
            },
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "headers": {
                "cache-control": "s-maxage=0"
            },
            "dest": "/index.html"
        }
    ]
}